@model IEnumerable<MVCProyect.Models.POCOS.User>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>

</div>
<div class="col-sm-3 text-left m-5">
    <div class="card"> 
        <div class="card-header">
            <h3 class="text-center">Users</h3>
        </div>
        <div class="card-body">
            <table class="table table-striped table-bordered">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Discount</th>
                </tr>
               

                </thead>
                <tbody>
                @foreach (var user in Model)
                 {
                     <tr>
                         <td>@Html.ActionLink(user.Name, "Details", "Users",new
                        {
                            id=user.Id
                        },null)</td>
                         <td>@user.MembershipType.DiscountRate%</td>
                     </tr>
                   
                 }
                
                </tbody>
            </table>
        </div>
    </div>
</div>

